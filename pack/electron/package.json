{
  "name": "redis-gui",
  "version": "1.6.3",
  "description": "A faster, better and more stable redis desktop manager.",
  "author": "RBFraphael",
  "private": true,
  "main": "electron-main.js",
  "dependencies": {
    "electron-updater": "4.6.5",
    "font-list": "^1.4.5"
  },
  "repository": "github:rbfraphael/redis-gui",
  "build": {
    "appId": "io.github.rbfraphael.redis-gui",
    "productName": "Redis GUI",
    "artifactName": "${name}.${version}.${ext}",
    "copyright": "Copyright Â© 2024 RBFraphael",
    "asar": true,
    "directories": {
      "output": "build-apps",
      "buildResources": "./"
    },
    "electronVersion": "12.2.3",
    "files": [
      "!static/js/*.map",
      "!static/css/*.map",
      "!*.map"
    ],
    "publish": [{
      "provider": "github",
      "owner": "rbfraphael",
      "repo": "redis-gui",
      "releaseType": "prerelease"
    }],
    "win": {
      "icon": "icons/icon.ico",
      "target": [
        "dir"
      ]
    }
  }
}
